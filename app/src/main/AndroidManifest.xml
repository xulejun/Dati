<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kincai.test"
    android:versionCode="1"
    android:versionName="1.0" >

    <!--允许联网 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!--获取GSM（2g）、WCDMA（联通3g）等网络状态的信息  -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!--获取wifi网络状态的信息 -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!--保持CPU 运转，屏幕和键盘灯有可能是关闭的,用于文件上传和下载 -->
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <!--获取sd卡写的权限，用于文件上传和下载-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--允许读取手机状态 用于创建BmobInstallation-->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />


    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:name="com.kincai.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/sys_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.kincai.test.StartActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.kincai.test.MainActivity"></activity>
        <activity android:name="com.kincai.test.LoginActivity"></activity>
        <activity android:name="com.kincai.test.NavigationActivity"></activity>
        <activity android:name="com.kincai.test.ResultActivity"></activity>
        <activity android:name="com.kincai.test.helpActivity"></activity>
        <activity android:name="com.kincai.test.AboutActivity"></activity>
        <activity android:name="com.kincai.test.RegisterActivity"></activity>
    </application>

</manifest>