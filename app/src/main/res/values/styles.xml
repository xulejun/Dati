<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    
    <style name="main_RadioButton_bottom">
        
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:button">@null</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    
    <style name="main_tab_bottom">
        <item name="android:textSize">13.0dp</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@drawable/down_menu_bg</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">@color/color3</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:button">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    
    <style name="Transparent">  
	    <item name="android:windowBackground">@color/background</item>  
	    <item name="android:windowIsTranslucent">true</item>    
	    <item name="android:windowAnimationStyle">@+android:style/Animation.Translucent</item>  
  </style>  
  
    
    <style name="result_trem_tv">
        <item name="android:layout_weight">1.0</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/shiliu</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">3dp</item>
    </style>
    
    <style name="result_tv">
        <item name="android:layout_weight">1.0</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/ershi</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:textColor">@color/whiles</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
    <style name="androidtest_title_tv">
        <item name="android:textColor">@color/whiles</item>
        <item name="android:padding">5dp</item>
        <item name="android:textSize">@dimen/shiba</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/radio_title_shape</item>
    </style>
    
    <style name="submit_bt">
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:padding">5dp</item>
        <item name="android:textSize">@dimen/shiba</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/login_btn</item>
    </style>
    
    <style name="result_button">
        <item name="android:layout_weight">1.0</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/shiba</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:background">@drawable/login_btn</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">5dp</item>
    </style>
    
    <style name="home_down_menu">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:textColor">#ffffff00</item>
	    <item name="android:textSize">@dimen/ershi</item> 
    </style>
    
    <style name="AnimBottom" parent="@android:style/Animation">  
    	<item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
	</style>
	<style name="menu_btn">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:textColor">@color/menu_btn</item>
        <item name="android:background">@drawable/menu_btn</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="radio_check">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
    </style>
    <style name="topic_title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">17sp</item>
    </style>
    <style name="title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textColor">@color/title</item>
        <item name="android:textSize">@dimen/ershi</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
    </style>
    
    <style name="login_edittext">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/shape</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:padding">12dp</item>
        <item name="android:drawablePadding">20dip</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:paddingLeft">20dip</item>
        <item name="android:button">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
    </style>
</resources>